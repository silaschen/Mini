page{
  background-color:#f5f5f5;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
  font-family: "微软雅黑",helvetica neue,tahoma,arial,hiragino sans gb,microsoft yahei,sans-serif;
  box-sizing: border-box;
}
.wrap { width: 100%;background: #fff;padding: 0;margin: 0;} 
.wrap .title{width: 100%;line-height: 45px;text-align: center;clear: both;overflow: hidden;color: #333;}
.wrap .title .icon{display: inline-block;color: #6aacfa}
.text-center{text-align: center}
.text-gray{color: #666}
.text-blue{color: #0086D0}
.text-green{color: #48C23D}
.upper{text-transform:Uppercase;}
.label-green{background: #48C23D;color: #fff;padding: 2px 3px;border-radius: 3px;height: 13px;display: inline-block;line-height: 13px;font-size: 21rpx;margin-top: 3px;}
.label-blue{background: #0086D0;color: #fff;padding: 2px 3px;border-radius: 3px;height: 13px;display: inline-block;line-height: 13px;font-size: 21rpx;margin-top: 3px;}
.label-yellow{background: #f0ad4e;color: #fff;padding: 2px 3px;border-radius: 3px;height: 13px;display: inline-block;line-height: 13px;font-size: 21rpx;margin-top: 3px;}
.label-gray{background: #ddd;color: #666;padding: 2px 3px;border-radius: 3px;height: 13px;display: inline-block;line-height: 13px;font-size: 21rpx;margin-top: 3px;}
.pull-left{float: left;}
.pull-right{float: right}

.box{background: #fff;line-height: 35px;display: block;color: #333;margin: 5px;padding:5px;border:1rpx solid #eee;}

.form-group{margin-bottom: 10px;overflow: hidden}
.form-group .addon{width: 18%;float: left;overflow: hidden;line-height: 40px;font-size: 32rpx;text-indent: 10rpx;}
.form-group input{font-size: 32rpx;height: 40px;}
.form-control{border: 1rpx solid #ddd;padding: 5px 8px;line-height: 35px;height: 35px;background: #fff;}
.footer{font-size: 25rpx;color: #ccc;text-align: center;width: 100%;margin: 20px auto;padding-bottom:120rpx;overflow: hidden;display: block}
/*0.5px底部*/
.tinyline{ position:relative;clear: both;display: block;width: 100%;}
.tinyline:before{
content: '';
position: absolute;
width: 200%;
height: 1px;
bottom: 0;
border-bottom: 1px solid #dedede;
-webkit-transform-origin: 0 0;
transform-origin: 0 0;
-webkit-transform: scale(.5,.5);
transform: scale(.5,.5);
-webkit-box-sizing: border-box;
box-sizing: border-box
}

/*重写按钮样式*/
button,button::after{border-radius: 0;}
button[type="default"] {color:#999;}
button[type="primary"] {
  background: #129FE7;
}
.button-hover[type="primary"]{
  background: #0E89C8;
}
button:after{border-radius: 0;width: 200%;  
    height:200%;  
    transform: scale(.5, .5);  
}
.clear:after{
  content:"";
  clear:both;
  display:block;
  height:0;
  overflow:hidden;
  visibility:hidden;
}
.fontcolor{
  color:#129FE7;
}